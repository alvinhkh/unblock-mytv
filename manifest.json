{
	"version": "1.0.0",

	"name": "__MSG_extension_name__",
	"description": "__MSG_extension_description__",

	"default_locale": "en",
	"manifest_version": 2,
	"minimum_chrome_version": "34.0",
	
	"permissions": [
		"storage",
		"declarativeWebRequest",
		"<all_urls>"
	],

	"options_page": "options.html",
	"icons": {
		"128": "icon.png"
	},
	
	"background": {
		"scripts": [
			"event_page.js"
		],
		"persistent": false
	}
}
